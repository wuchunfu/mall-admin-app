/* pages/tabbar/cart/cart.wxss */
page{
  background-color: #F2F2F2;
}
.goods-card{
  width: 94%;
  height: auto;
  margin: 2% 3%;
  border-radius: 5px;
  background-color: #FFFFFF;
}
.van-swipe-cell__left,
.van-swipe-cell__right {
  display: flex;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  flex-direction: column;
  align-items: center;
  box-sizing: content-box;
  color: #fff;
  justify-content: center;
  border-radius: 5px 0px 0px 5px;
  background-color: #f44;
}
.submit-bar{
  margin-bottom: 50px;
}